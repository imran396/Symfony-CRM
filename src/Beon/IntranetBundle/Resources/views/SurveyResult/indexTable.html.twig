<table id="indexTable" class="table">
    <thead>
    <tr>
        <th>{% trans %}Id{% endtrans %}</th>
        <th>{% trans %}Date{% endtrans %}</th>
        <th>{% trans %}Customer{% endtrans %}</th>
        <th>{% trans %}Actions{% endtrans %}</th>
    </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td>{{ entity.id }}</td>
            <td>{% if entity.date %}{{ entity.date|date(defaultDateFormat) }}{% endif %}</td>
            <td>{{ entity.customer.name }}</td>
            <td>
               <a class="btn btn-table-actions" href="{{ path('surveyresult_show', { 'id': entity.id }) }}">{% trans %}show{% endtrans %}</a>
               <a class="btn btn-table-actions" href="{{ path('surveyresult_edit', { 'id': entity.id }) }}">{% trans %}edit{% endtrans %}</a>
            </td>
        </tr>
    {% endfor %}
    </tbody>
</table>
