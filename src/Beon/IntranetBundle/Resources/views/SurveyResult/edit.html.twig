{% extends 'IntranetBundle::mainLayout.html.twig' %}

{% block content %}
{% form_theme edit_form 'IntranetBundle:Form:cp_form_theme.html.twig' %}
<div class="row-fluid">
        <div class="block span12">
            <div class="block-heading" data-toggle="collapse" data-target="#tablewidget">{% trans %}Surveyresult edit{% endtrans %}</div>
            <div id="widget1container" class="block-body collapse in">

                
		{{ form_start(edit_form) }}

                {{form_row(edit_form.customer)}}
                {{form_row(edit_form.date)}}
                {{form_row(edit_form.timeIn)}}
                {{form_row(edit_form.timeOut)}}
                
                <div class="radio-main">
		  {{form(edit_form)}}
                </div>
                
                {{form_row(edit_form.submit)}}
                {{ form_end(edit_form) }}

                <ul class="record_actions unstyled">
                    <li>
                        <a onclick="return confirmation()" class="btn btn-table-actions" href="{{ path('surveyresult') }}">
                            {% trans %}Back to the list{% endtrans %}
                        </a>
                        <a onclick="return confirmation()" class="btn btn-table-actions"  href="{{ path('surveyresult_show', { 'id': entity.id }) }}"> {% trans %}show{% endtrans %}</a>
                    </li>
                </ul>
            </div>
        </div>
    </div>
{% endblock %}

