
<table id="indexTable" class="table">
        <thead>
            <tr>
                <th> {% trans %}Id{% endtrans %}</th>
                <th> {% trans %}Name{% endtrans %}</th>
                <th> {% trans %}Population{% endtrans %}</th>
                <th> {% trans %}Actions{% endtrans %}</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td>{{ "CT%05d"|format(entity.id) }}</td>
                <td>{{ entity.name }}</td>
                <td>{{ entity.population|number_format(0, ',') }}</td>
                <td>
                 <div class="inline-forms">
                        <a  class="btn btn-table-actions"  href="{{ path('city_show', { 'id': entity.id }) }}"> {% trans %}show{% endtrans %}</a>
                        <a  class="btn btn-table-actions"  href="{{ path('city_edit', { 'id': entity.id }) }}"> {% trans %}edit{% endtrans %}</a>
                </div>
                </td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

