{% if timetrackingEntities|length  %}
	<div class="clear">&nbsp;</div>
	<div class="row-fluid">
		<div class="block span12">
			<div class="block-heading">
			      {%trans%}Time Tracking{%endtrans%}
			</div>
			<table id="indexTable" class="table">
				<thead>
					<tr>
						<th>{% trans %}Id{% endtrans %}</th>
						<th>{% trans %}User Name{% endtrans %}</th>
						<th>{% trans %}Day{% endtrans %}</th>
						<th>{% trans %}Hours{% endtrans %}</th>
						<th>{% trans %}Stakeholder{% endtrans %} / {% trans %}Task{% endtrans %}</th>
						<th>{% trans %}Actions{% endtrans %}</th>
					</tr>
				</thead>
				<tbody>
					{% for entity in timetrackingEntities %}
						<tr>
							<td>{{ "TT%05d"|format(entity.id) }}</td>
							<td><a href="{{ path('user_show', { 'id': entity.user.id }) }}">{{ entity.user }}</td>
							<td>{% if entity.day %}{{ entity.day|date(defaultDateFormat) }}{% endif %}</td>
							<td>{{ entity.hours }}</td>
							<td>{{ entity.task|default(entity.customer) }}</td>
							<td>
							<div class="inline-forms">
								<a  class="btn btn-table-actions"  href="{{ path('timetracking_show', { 'id': entity.id }) }}">{% trans %}show{% endtrans %}</a>
								<a  class="btn btn-table-actions"  href="{{ path('timetracking_edit', { 'id': entity.id }) }}">{% trans %}edit{% endtrans %}</a>
							</div>
							</td>
						</tr>
					{% endfor %}
				</tbody>
			</table>
		</div>
	</div>
{%endif%}