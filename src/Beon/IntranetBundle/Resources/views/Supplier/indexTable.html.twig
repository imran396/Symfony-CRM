<table id="indexTable" class="table">
    <thead>
    <tr>
        <th>{% trans %}Id{% endtrans %}</th>
        <th>{% trans %}Name{% endtrans %}</th>
        <th>{% trans %}Audience size{% endtrans %}</th>
        <th>{% trans %}Type{% endtrans %}</th>
        <th>{% trans %}Actions{% endtrans %}</th>
    </tr>
    </thead>
    <tbody>
    {% for entity in entities %}
        <tr>
            <td>{{ "PD%05d"|format(entity.id) }}</td>
            <td>{{ entity.name }}</td>
            <td>{{ entity.audiencesize }}</td>
            <td>{{ entity.supplierType }}</td>
            {% block actionsRows %}
            <td>
                <a class="btn btn-table-actions" href="{{ path('supplier_show', { 'id': entity.id }) }}">{% trans %}show{% endtrans %}</a>
                <a class="btn btn-table-actions" href="{{ path('supplier_edit', { 'id': entity.id }) }}">{% trans %}edit{% endtrans %}</a>
            </td>
            {% endblock %}
        </tr>
    {% endfor %}
    </tbody>
</table>
