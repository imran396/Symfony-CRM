<table id="indexTable" class="table">
        <thead>
            <tr>
                <th>Authtoken</th>
                <th>Expires At</th>
                <th>Created At</th>
            </tr>
        </thead>
        <tbody>
        {% for entity in entities %}
            <tr>
                <td>{{ entity.authToken|truncate(40) }}</td>
                <td>{{ entity.expiresAt|date(defaultDateFormat) }}</td>
                <td>{% if entity.createdAt %}{{ entity.createdAt|date(defaultDateFormat) }}{% endif %}</td>
            </tr>
        {% endfor %}
        </tbody>
   </table>